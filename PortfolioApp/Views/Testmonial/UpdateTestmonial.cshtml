@model PortfolioApp.Models.Entity.TblTestmonial
@{
    ViewBag.Title = "UpdateTestmonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="text-center">
    <h2>Add New Testmonial</h2>
</div>



<form method="post">

    <label>Testmonial ID</label>
    <input type="text" class="table table-bordered" readonly value="@Model.TestmonialId" name="TestmonialId" />
    <label>Name Surname</label>
    </br>
    @Html.TextBoxFor(x => x.NameSurname, new { @class = "form-control" })

    <label>Description</label>
    </br>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })

    <label>Image url</label>
    </br>
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })



    <button class="btn btn-info" type="submit">Submit</button>
    <a href="/Testmonial/Index/" class="btn btn-outline-warning">Go Back</a>
</form>


