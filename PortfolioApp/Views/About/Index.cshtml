@model List<PortfolioApp.Models.Entity.TblAbout>

    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/AdminLayout/Index.cshtml";
    }

    <h2>Index</h2>

    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Header</th>
            <th>Description</th>
            <th>ImageUrl</th>

            <th>Update</th>
            @*<th>Delete</th>*@
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AboutID</td>
                <td>@item.Title</td>
                <td>@item.Header</td>
                <td>@item.Description</td>
                <td>@item.Header</td>

                <td> <a href="/About/UpdateAbout/@item.AboutID" class="btn btn-outline-success">Update</a></td>
                @*<td> <a href="/Category/DeleteAbout/@item.AboutID" class="btn btn-outline-danger">Delete</a></td>*@

            </tr>

        }

    </table>
